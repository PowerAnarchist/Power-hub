ThirdSection:NewToggle("Auto Drink", "Auto Drinks juices", function(state)
    if state then
        _G.Using = true
while _G.Using do
local Players = game:GetService("Players")
local Lp = Players.LocalPlayer
local Character = Lp.Character or Lp.CharacterAdded:Wait()
local Humanoid = Character.Humanoid
local user = tostring(game.Players.LocalPlayer.Name)
for i,v in pairs(Lp.Backpack:GetChildren()) do 
    if v:IsA("Tool") and string.find(v.Name, "Fruit Juice") or string.find(v.Name, "Apple Juice") or string.find(v.Name, "Banana Juice") or string.find(v.Name, "Pumkin Juice") or string.find(v.Name, "Coconut Milk") or string.find(v.Name, "Sour Juice") or string.find(v.Name, "Cider+") or string.find(v.Name, "Juice+") or string.find(v.Name, "Smoothie+") or string.find(v.Name, "Lemonade+") then  
        Humanoid:EquipTool(v)
        v:Activate()
        wait()
    end 
end 
end
else
        _G.Using = false
    end
end)

local FourthSection = Tab4:NewSection("Select a place to Teleport to")

ThirdSection:NewToggle("Destroy tools", "Destroys tools to grind tea", function(state)
    if state then
        _G.Drinking = true
while _G.Drinking do
    wait()
local Players = game:GetService("Players")
local Lp = Players.LocalPlayer
local Character = Lp.Character or Lp.CharacterAdded:Wait()
local Humanoid = Character.Humanoid
for i,v in pairs(Lp.Backpack:GetChildren()) do 
    if v:IsA("Tool") and string.find(v.Name, "Black") or string.find(v.Name, "Flintlock") or string.find(v.Name, "Kogatana") then 
        Humanoid:EquipTool(v)
        v:Destroy()
        wait()
    end 
end 
end
    else
        _G.Drinking = false
    end
end)

ThirdSection:NewToggle("Dupe Tea", "Dupes tea for grinding purpose", function(state)
    if state then
        _G.Dupe = true
while _G.Dupe do 
wait(0)
game:GetService("Players").LocalPlayer.Character.Weapons:FireServer()
end
    else
       _G.Dupe = false
    end
end)

ThirdSection:NewToggle("Auto Drink Tea", "drinking", function(state)
    if state then
        _G.Sip = true
while _G.Sip do
    wait()
local Players = game:GetService("Players")
local Lp = Players.LocalPlayer
local Character = Lp.Character or Lp.CharacterAdded:Wait()
local Humanoid = Character.Humanoid
for i,v in pairs(Lp.Backpack:GetChildren()) do 
    if v:IsA("Tool") and string.find(v.Name, "Tea Cup")  then 
        Humanoid:EquipTool(v)
        v:Activate()
        wait(0.5)
        game:GetService'Players'.LocalPlayer.Character:FindFirstChildOfClass'Humanoid':UnequipTools()
    end 
end 
end
    else
        _G.Sip = false
    end
end)

